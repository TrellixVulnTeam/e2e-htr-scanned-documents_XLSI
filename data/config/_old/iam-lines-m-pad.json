{
  "dataset": "iam",
  "subset": "lines",
  "limit": -1,
  "shuffle": true,
  "test_frac": 0.1,
  "dev_frac": 0.1,
  "invert": true,
  "threshold": false,
  "crop": true,
  "scale": {
    "mode": "height",
    "size": [-1, 60]
  },
  "padding": [5, 5, 60, 5],
  "warp": {
    "num": 5,
    "gridsize": [30, 30],
    "deviation": 2.7
  },
  "binarize": false
}
