{
  "dataset": "iam",
  "subset": "lines",
  "limit": 30,
  "shuffle": true,
  "test_frac": 0.1,
  "dev_frac": 0.1,
  "invert": false,
  "threshold": false,
  "crop": true,
  "scale": {
    "mode": "height",
    "size": [-1, 80]
  },
  "padding": 5,
  "warp": {
    "num": 5,
    "gridsize": [30, 30],
    "deviation": 2.7
  },
  "binarize": false
}
