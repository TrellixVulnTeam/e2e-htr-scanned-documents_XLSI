{
  "dataset": "iam",
  "subset": "lines",
  "limit": -1,
  "shuffle": true,
  "test_frac": 0.1,
  "dev_frac": 0.1,
  "invert": true,
  "threshold": false,
  "crop": true,
  "scale": {
    "mode": "height",
    "size": [-1, 60]
  },
  "padding": 0,
  "warp": {
    "num": 10,
    "gridsize": [20, 20],
    "deviation": 3.0
  },
  "binarize": false
}
