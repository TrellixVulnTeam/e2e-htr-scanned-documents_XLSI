{
  "blocks": [
    {
      "type": "ParagraphSegmentation",
      "extractor": {
        "sauvola_window": 19,
        "erode_kernel": [35, 35],
        "dilate_kernel": [35, 35],
        "scaling": 5,
        "expand": 0
      }
    },
    {
      "type": "LineSegmentation",
      "extractor": {
        "scale_factor": 5.0,
        "erode_kernel": [1, 10],
        "dilate_kernel": [1, 10],
        "relative_maxima_dist": 35,
        "astar": {
          "c": {
            "d": 60,
            "d2": 0,
            "m": 275,
            "v": 3.5,
            "n": 1
          }
        }
      }
    }
  ],
  "data": {
    "path": "../paper-notes/data/final/test/",
    "ending": "-stripped.png",
    "limit": 20
  },
  "viz": {
    "type": "RegionVisualizer",
    "store": "./e2eoutputs/linedist"
  }
}
