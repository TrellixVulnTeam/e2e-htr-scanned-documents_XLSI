{
  "blocks": [
    {
      "type": "ParagraphSegmentation",
      "extractor": {
        "sauvola_window": 19,
        "erode_kernel": [35, 35],
        "dilate_kernel": [35, 35],
        "scaling": 5,
        "expand": 0
      }
    },
    {
      "type": "LineSegmentation",
      "extractor": {
        "scale_factor": 5.0,
        "relative_maxima_dist": 35,
        "astar": {
          "c": {
            "d": 130,
            "d2": 50,
            "m": 150,
            "v": 3.5,
            "n": 1
          }
        }
      }
    }
  ],
  "gt": {
    "type": "LineRegion",
    "viz": true
  },
  "eval": [
    {
      "type": "IoUPixelSum",
      "filter_class": false
    }
  ],
  "data": {
    "path": "../paper-notes/data/final/dev/",
    "ending": "-stripped.png",
    "limit": 200
  },
  "ranger": {
    "paths": [
      ["blocks", 1, "extractor", "erode_kernel", 1],
      ["blocks", 1, "extractor", "dilate_kernel", 1]
    ],
    "values": {
      "from": 10,
      "to": 30,
      "step": 4
    },
    "template": "line dist {}"
  }
}
