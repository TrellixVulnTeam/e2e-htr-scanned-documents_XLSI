{
  "blocks": [
    {
      "type": "ParagraphDetection",
      "extractor": {
        "sauvola_window": 19,
        "erode_kernel": [35, 35],
        "dilate_kernel": [35, 35],
        "scaling": 5,
        "expand": 0
      }
    }
  ],

  "data": {
    "path": "./e2eoutputs/steps/step1",
    "suffix": "-paper.png",
    "limit": 0
  },
  "viz": {
    "type": "RegionVisualizer",
    "store": "./e2eoutputs/steps/step2",
    "filled": true
  }
}
