{
  "blocks": [
    {
      "type": "WordSegmentation",
      "extractor": {
        "cluster_y_eps": 0.1
      }
    }
  ],
  "gt": {
    "type": "WordRegion",
    "viz": true
  },
  "eval": [
    {
      "type": "IoU",
      "filter_class": false
    }
  ],
  "data": {
    "path": "../paper-notes/data/final/dev/",
    "ending": "-stripped.png",
    "limit": 500
  },
  "ranger": {
    "paths": [["blocks", 0, "extractor", "cluster_y_eps"]],
    "values": {
      "from": 0.01,
      "to": 0.2,
      "step": 0.01
    },
    "template": "kernel {}"
  }
}
